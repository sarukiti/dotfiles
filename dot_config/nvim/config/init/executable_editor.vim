set number
set tabstop=4
set shiftwidth=4
set laststatus=2
set backspace=indent,eol,start

inoremap <silent> jk <ESC>
inoremap { {}<LEFT>
inoremap [ []<LEFT>
inoremap ( ()<LEFT>
inoremap " ""<LEFT>
inoremap ' ''<LEFT>
nnoremap <silent> te :tabedit<cr>
nnoremap <silent> tq :tabclose<cr>
nnoremap <silent> tn gt<cr>
nnoremap <silent> tb gT<cr>
